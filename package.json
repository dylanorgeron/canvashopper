  "dependencies": {
    "@types/ws": "^8.5.3",
    "color-string": "^1.5.5",
    "eventemitter3": "^3.0.1",
    "glob-parent": "5.1.2",
    "nth-check": "^2.0.1",
    "parcel": "2.0.1",
    "parcel-bundler": "^1.5.1",
    "postcss": "^7.0.36",
    "postcss-cssnext": "^3.1.0",
